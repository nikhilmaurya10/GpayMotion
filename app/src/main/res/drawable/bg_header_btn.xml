<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape xmlns:android="http://schemas.android.com/apk/res/android">
        <solid android:color="#E7F0FD"/>

        <stroke android:width="3dp"
            android:color="#E7F0FD"
            />

        <padding android:left="1dp"
            android:top="1dp"
            android:right="1dp"
            android:bottom="1dp"
            />

        <corners
            android:topLeftRadius="26dp"
            android:topRightRadius="26dp"
            android:bottomLeftRadius="26dp"
            android:bottomRightRadius="26dp"/>
    </shape>
</item>
    <item android:height="20dp"
        android:width="20dp"
        android:start="16dp"
        android:gravity="center_vertical">
        <bitmap android:src="@drawable/shoppingbag"
            android:tint="#276ECD">

        </bitmap>
    </item>
</layer-list>
